dorulesfile()
rule('builtin.lua', 'luac -o %o %f', '%B.luac')
rule('builtin.luac', 'xxd -i %f %o', 'luabuiltin.h')
bang_cc('parser.c', 'luabuiltin.h')
bang_mingwcc('parser.c', 'luabuiltin.h')
for index, file in ipairs(glob('*.c'))
do 
	if file ~= 'parser.c' then 
		bang_cc(file)
		bang_mingwcc(file)
	end
end
