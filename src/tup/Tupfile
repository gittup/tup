tup.dorulesfile()
tup.definerule{inputs = {'builtin.lua'}, outputs = {'luabuiltin.h'}, command = './generate-luabuiltin.sh > luabuiltin.h'}
bang_cc('parser.c', 'luabuiltin.h')
bang_mingwcc('parser.c', 'luabuiltin.h')
for index, file in ipairs(tup.glob('*.c'))
do 
	if file ~= 'parser.c' then 
		bang_cc(file)
		bang_mingwcc(file)
	end
end
